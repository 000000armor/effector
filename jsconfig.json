{
 "compilerOptions": {
  "target": "esnext",
  "module": "es2015",
  "experimentalDecorators": true,
  "allowSyntheticDefaultImports": true,
  "skipLibCheck": false,
  "noLib": false,
  "skipDefaultLibCheck": true,
  "alwaysStrict": true,
  "strict": true,
  "baseUrl": "src",
  "paths": {
   "^effector$": ["./index.js"],
   "^effector/(.+)": ["./$1"]
  }
 },
 "typeAcquisition": {
  "enable": true
 },
 "include": ["src"]
}
