[ignore]
<PROJECT_ROOT>/lib/*
<PROJECT_ROOT>/es/*
<PROJECT_ROOT>/node_modules/jss/*
<PROJECT_ROOT>/node_modules/most/*
<PROJECT_ROOT>/node_modules/fbjs/*
.*/dist/.*
.*/logs/.*
.*/examples/.*
.*/.vscode/.*
.*/typings/.*
.*/**/*.draft.js
**/lib/*.flow
.*/**/es/*.flow

[include]
./src

[lints]
all=warn
untyped-type-import=error

[libs]
./flow-typed

[options]
include_warnings=true
max_header_tokens=5
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
experimental.const_params=true
suppress_comment=\\(.\\|\n\\)*\\$FlowIssue
suppress_comment=\\(.\\|\n\\)*\\$off
suppress_type=$FlowIssue
suppress_type=$off
emoji=true
esproposal.decorators=ignore
module.use_strict=true
esproposal.export_star_as=enable
module.name_mapper='^most-subject$' -> '<PROJECT_ROOT>/node_modules/most-subject'
module.name_mapper='^fp-ts\(.*\)$' -> '<PROJECT_ROOT>/node_modules/fp-ts\1'
module.name_mapper='^monocle-ts\(.*\)$' -> '<PROJECT_ROOT>/node_modules/monocle-ts\1'
module.name_mapper='^\(.*\)redux\(.*\)$' -> '<PROJECT_ROOT>/node_modules/\1redux\2'
module.system.node.resolve_dirname=<PROJECT_ROOT>/node_modules
