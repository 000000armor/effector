{
  "name": "effector",
  "version": "0.11.0",
  "description": "Redux effects",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "jest --config=jest.json",
    "prepublishOnly": "npm run build && npm run flow-copy",
    "flow-copy:cjs": "flow-copy-source -i=__test__**\/* -i=joint* -i=mill* -i=subject* src lib && cp src\/*flow lib",
    "flow-copy:es": "flow-copy-source -i=__test__**\/* -i=joint* -i=mill* -i=subject* src es && cp src\/*flow es",
    "flow-copy": "npm run flow-copy:cjs && npm run flow-copy:es",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ --ignore src/__test__ -d lib -s --delete-dir-on-start",
    "build:es": "cross-env BABEL_ENV=es babel src/ --ignore src/__test__ -d es -s --delete-dir-on-start",
    "test:ff": "jest --config=jest-fastflow.json --watch",
    "test:watch": "jest --config=jest.json --watch",
    "flow": "pm2 start flow-effector.sh",
    "flow:status": "flow status",
    "flow:stop": "pm2 stop flow-effector"
  },
  "author": "Zero Bias",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/node": "^7.0.0-beta.40",
    "@babel/plugin-proposal-async-generator-functions": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.40",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.40",
    "@babel/plugin-transform-block-scoping": "^7.0.0-beta.40",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.40",
    "@babel/plugin-transform-for-of": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.45.0",
    "eslint-plugin-jest": "^21.12.2",
    "flow-bin": "^0.66.0",
    "flow-copy-source": "^1.3.0",
    "flow-typed": "^2.3.0",
    "fluture": "^8.0.2",
    "fs-extra": "^5.0.0",
    "immutable": "^4.0.0-rc.9",
    "jest": "^22.4.0",
    "js-yaml": "^3.10.0",
    "pm2": "^2.10.1",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
    "prettier-eslint-cli": "^4.7.1",
    "redux": "^3.7.2",
    "redux-act": "^1.7.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "most": "^1.7.3",
    "most-subject": "5.3.0",
    "symbol-observable": "^1.2.0"
  },
  "peerDependencies": {
    "redux": "^3.7.2"
  },
  "sideEffects": false,
  "files": [
    "es",
    "lib",
    "src",
    "index.d.ts"
  ],
  "repository": "https://gitlab.com/zerobias/effector"
}
