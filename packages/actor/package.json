{
 "name": "@effector/actor",
 "version": "0.0.1",
 "main": "src/index.js",
 "license": "MIT",
 "scripts": {
  "test": "jest --config=jest.json",
  "prepublishOnly": "npm run clean && npm run build",
  "build": "npm run build:es && npm run build:cjs",
  "build:cjs":
   "BABEL_ENV=commonjs ./node_modules/.bin/babel src/ --ignore __tests__ -d lib -s",
  "build:es": "./node_modules/.bin/babel src/ --ignore __tests__ -d es -s",
  "clean": "./node_modules/.bin/rimraf lib/ && ./node_modules/.bin/rimraf es/",
  "watch": "npm run build:es -- --watch"
 },
 "dependencies": {
  "invariant": "^2.2.4",
  "@effector/warning": "^0.0.1"
 },
 "devDependencies": {
  "@babel/cli": "^7.0.0-beta.44",
  "@babel/core": "^7.0.0-beta.44",
  "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
  "@babel/preset-env": "^7.0.0-beta.44",
  "@babel/preset-flow": "^7.0.0-beta.44",
  "babel-core": "7.0.0-bridge.0",
  "babel-jest": "^22.4.3",
  "cross-env": "^5.1.4",
  "fs-extra": "^5.0.0",
  "jest": "^22.4.3",
  "rimraf": "^2.6.2"
 },
 "files": ["src"]
}
