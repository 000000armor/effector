{
 "env": {
  "commonjs": {
   "plugins": ["@babel/plugin-transform-modules-commonjs"]
  },
  "test": {
   "plugins": ["@babel/plugin-transform-modules-commonjs"]
  }
 },
 "presets": [
  "@babel/preset-flow",
  [
   "@babel/preset-env",
   {
    "modules": false,
    "shippedProposals": true,
    "targets": {
     "node": "current"
    }
   }
  ]
 ],
 "plugins": ["@babel/plugin-proposal-class-properties"]
}
