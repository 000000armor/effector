//@flow

import type {Cmd} from '../cmd'
import * as Name from './type'

class Single {
 +type: Name.SingleType;
 +data: Cmd;
}
class Multi {
 +type: Name.MultiType;
 +data: Set<Step>;
}
class Seq {
 +type: Name.SeqType;
 +data: Array<Step>;
}

export type Step = Single | Multi | Seq
export type {Single, Multi, Seq}

declare export function single(data: Cmd): Single

declare export function multi(): Multi

declare export function seq(data: Array<Step>): Seq
