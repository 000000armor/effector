{
  "version": 2,
  "github": {
    "silent": true
  },
  "alias": ["effector.now.sh"],
  "routes": [
    {
      "src": "/try/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://share.effector.dev/$1"
      }
    },
    {
      "src": "/docs/api/effector",
      "status": 301,
      "headers": {
        "Location": "https://effector.dev/docs/api/effector/effector"
      }
    },
    {
      "src": "/en/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://effector.dev/docs/$1"
      }
    },
    {
      "src": "/(.*)",
      "status": 301,
      "headers": {
        "Location": "https://effector.dev/$1"
      }
    }
  ]
}
